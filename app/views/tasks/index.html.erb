<h1>タスク一覧</h1>

<table>
  <tr>
    <th>タイトル</th>
    <th>内容</th>
    <th>終了期限</th>
    <th>優先順位</th>
    <th>状態</th>
  </tr>

<% @tasks.each do |task| %>
  <tr>
    <td><%= task.title %></td>
    <td><%= task.content %></td>
    <td><%= task.deadline %></td>
    <td><%= task.priority %></td>
    <td><%= task.status %></td>
  </tr>
<% end %>
</table>